<generatorConfiguration>
    <classPathEntry location="D:\software\apache-maven-3.5.3-bin\repository\mysql\mysql-connector-java\5.1.46\mysql-connector-java-5.1.46.jar"/>
    <context id="my" targetRuntime="MyBatis3">
